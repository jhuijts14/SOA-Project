<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/CardProcessing" xmlns:tns="http://www.example.org/CardProcessing" elementFormDefault="qualified">

    
    
    



    


    <element name="Operation" type="tns:OperationType"></element>
    

    <simpleType name="OperationType">
    	<restriction base="string">
    		<enumeration value="Credit"></enumeration>
    		<enumeration value="Debit"></enumeration>
    		<enumeration value="Void"></enumeration>
    	</restriction>
    </simpleType>

    <complexType name="AuthorizationRequestType">
    	<choice>
    		<element name="MerchantAccount" type="string"></element>
    		<element name="CustomerPIN" type="int"></element>
    	</choice>
    </complexType>
    
    <element name="AuthenticationRequest"
    	type="tns:AuthorizationRequestType">
    </element>

    <element name="Response" type="tns:ResponseType"></element>
    
    <simpleType name="ResponseType">
    	<restriction base="string">
    		<enumeration value="Authorized"></enumeration>
    		<enumeration value="Declined"></enumeration>
    		<enumeration value="Void"></enumeration>
    	</restriction>
    </simpleType>

    <element name="Authentication" type="tns:AuthenticationType"></element>
    
    <simpleType name="AuthenticationType">
    	<restriction base="string">
    		<enumeration value="Merchant"></enumeration>
    		<enumeration value="Customer"></enumeration>
    	</restriction>
    </simpleType>

</schema>